{
  "name": "node-console-template",
  "version": "0.1.0",
  "description": "",
  "main": "dist/script.js",
  "scripts": {
    "start-sample": "tsc && node dist/script.js test-data/input-file.txt test-data/output-file.txt",
    "start-sample-ts": "node_modules/.bin/ts-node src/script.ts test-data/input-file.txt test-data/output-file.txt",
    "test": "tsc && mocha dist/test/**/*.js",
    "test-watch": "mocha-typescript-watch",
    "prod:install": "npm install --production",
    "prod:build": "tsc",
    "prod:start": "NODE_ENV=production node dist/script.js",
    "fix-eslint-all": "eslint --fix -c .eslintrc.json --ignore-pattern '**/node_modules/**' '**/src/**/*.ts'",
    "check-eslint-all": "eslint -c .eslintrc.json --ignore-pattern '**/node_modules/**' '**/src/**/*.ts'",
    "make-pretty": "prettier --write '**/src/**/*.{js,jsx,ts,tsx,md,html,css,json,scss}'"
  },
  "author": "Markus Peitl",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "chai": "^4.3.4",
    "copyfiles": "^2.4.1",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.19",
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0"
  },
  "dependencies": {
    "@types/argparse": "^2.0.10",
    "@types/node": "^16.6.2",
    "argparse": "^2.0.1",
    "node-fetch": "^2.6.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  }
}
